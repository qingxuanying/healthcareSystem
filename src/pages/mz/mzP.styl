.D_info
    width 100%
    height: 50%
    background-color: #FFFFFF
    .mzP_header
        // border: solid 3px black
        display: flex
        justify-content: space-between
        opacity: 1;
        padding: 3% 
        .mzP-h-b
            background-color: #1DABA6
            color: #FFFFFF
    .mzP_body
        padding: 2% 
        font-size: 16px
        background-color: #FFFFFF
        .mzP-table
            width: 100%
            .mzP-thead
                color: #9C9FA7
            .mzP-tbody
                padding-top: 1%
                .mzP-tbody-tr
                    padding-top: 1%
                    height: 50px
                    line-height: 50px
                    // border: solid 2px black
                    .mzP-tbody-circle
                        background-color: #E6E6E6
                        width: 18px
                        height: 18px
                        border-radius: 50%
                        margin: 0 auto
                    .mzP-ops
                        display: flex
                        // justify-content: space-between
                        .mzP-ico
                            width: 50%
